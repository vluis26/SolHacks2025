{
    "name": "SyllaByte",
    "description": "Automatically add your syllabus to your calendar.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html"
    },
    "permissions": ["identity", "storage"],

    "background": {
        "service_worker": "background.js"
    }
}
  